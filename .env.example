# Architect Command Center: Environment Template
# Copy this file to .env and fill in the values

# Google Gemini API Key - CRITICAL SECURITY: Only on server-side
# Obtain from https://aistudio.google.com/
# WARNING: Never expose this in client-side code
GEMINI_API_KEY=your_gemini_api_key_here

# Backend API URL (Default for development)
# This endpoint is used to proxy all API requests securely
VITE_API_URL=http://localhost:3001

# Optional: Server Port (Default: 3001)
# PORT=3001

# Optional: Application Environment
# NODE_ENV=development

# Optional: Enable Debug Logging
# VITE_LOG_LEVEL=debug  # debug|info|warn|error (default: info)

# Optional: Feature Flags
# VITE_ENABLE_ANALYTICS=false
# VITE_ENABLE_OFFLINE_MODE=false
